{
  "manifest_version": 2,
  "name": "CSS Injector",
  "version": "1.0",
  "description": "Injects custom CSS.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "CSS Injector"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
